# vim: set ft=dosini:

[core]
    excludesfile = ~/.gitignore

[alias]
    st = status
    ci = commit --verbose
    co = checkout
    br = branch
    ls = show --pretty=\"format:%h %s (%cn, %ar)\n-------\" --name-status
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    unstage = reset HEAD
[user]
    email = jesse.dhillon@grandroundshealth.com
    name = Jesse Dhillon
[push]
	default = simple
