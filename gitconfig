[core]
    excludesfile = ~/.gitignore

[alias]
    st = status
    ci = commit --verbose
    co = checkout
    br = branch
    ls = show --pretty=\"format:<%h> %s (%cn, %ar)\n---------\" --name-only
    unstage = reset HEAD
